# interfaces(5) file used by ifup(8) and ifdown(8)
auto lo
iface lo inet loopback

iface eth0 inet static
	address 10.0.0.3
	netmask 255.255.255.0
	gateway 10.0.0.9


iface wlan0 inet static
	address 10.0.0.4
	netmask 255.255.255.0
	gateway 10.0.0.9

iface eth2 inet dhcp

iface eth3 inet static
	address 10.0.0.10
	netmask 255.255.0.0
	gateway 10.0.0.1

iface eth4 inet dhcp

